{
  "language": "ts",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "entryFile": "app",
  "monorepo": true,
  "compilerOptions": {
    "plugins": ["@nestjs/swagger/plugin"],
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "modules": {
      "type": "library",
      "root": "libs/modules",
      "entryFile": "index",
      "sourceRoot": "libs/modules/src",
      "compilerOptions": {
        "tsConfigPath": "libs/modules/tsconfig.lib.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
